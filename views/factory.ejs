    <main id="mobile">
      <img id="factory-machine-mobile" src="/assets/images/factory/factory-machine-mobile.png" alt="Factory Machine">
      <img id="factory-belt-mobile" src="/assets/images/factory/factory-belt.png" alt="Factory Belt">
