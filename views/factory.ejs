            <img class="factory-belt-gear-desktop" src="/assets/images/factory/factory-belt-gear.png" alt="Factory Belt Gear">
          </section>
          <img id="factory-machine-tube-desktop" src="/assets/images/factory/factory-machine-tube.png" alt="Factory Machine Tube">
          <section id="minifigures-desktop">
            <img src="/assets/images/chen.png" alt="Chen Minifigure">
            <img src="/assets/images/cinder.png" alt="Cinder Minifigure">
            <img src="/assets/images/artic3.png" alt="Superman Minifigure">
            <img src="/assets/images/batman.png" alt="Batman Minifigure">
          </section>
